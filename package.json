    "@vue-office/excel": "^1.1.7",
    "animate.css": "^4.1.1",
    "ant-design-vue": "^3.2.20",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.4",
    "socket.io-client": "^4.7.1",
    "vue": "^3.3.4",
    "vue-plyr": "^7.0.0",
    "vue-router": "^4.2.2",
    "vue3-video-play": "1.3.1-beta.6",
    "vxe-table": "^4.4.2",
    "x-data-spreadsheet": "^1.1.9",
    "xe-utils": "^3.5.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@element-plus/eslint-config": "^0.0.20230510",
    "@tsconfig/node18": "^2.0.1",
    "@types/file-saver": "^2.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.18",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.3.2",
    "autoprefixer": "^10.4.14",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
